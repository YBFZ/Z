<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.z.Mapper.ClassifyMapper">
    <select id="queryRubbishList" resultType="Classify">
        select * from classify
    </select>
    <select id="queryRubbishById" resultType="Classify">
        select * from classify where cid = #{cid}
    </select>
</mapper>